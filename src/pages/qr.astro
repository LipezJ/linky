---
import Layout from '@/layouts/Layout.astro'
import ComingSoon from '@/components/pages/ComingSoon.astro'
import NeedAccount from '@/components/pages/NeedAccount.astro'

import { Auth } from '@/lib/auth'

const auth = Auth.isSignedIn(Astro.cookies);
---

<Layout title="Qr generator">
  <NeedAccount auth={auth} />
  {
    auth && <ComingSoon />
  }
</Layout>